{
  "name": "machadalo",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^3.6.1",
    "@material-ui/icons": "^3.0.1",
    "classnames": "^2.2.6",
    "form-data": "^2.3.3",
    "google-map-react": "^1.1.4",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.2",
    "mapsort": "^1.0.1",
    "material-ui-tree": "^1.0.11",
    "moment": "^2.29.2",
    "node-sass-chokidar": "^1.3.3",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.14.2",
    "rc-datetime-picker": "^1.6.1",
    "react": "^16.4.2",
    "react-datetime": "^2.16.3",
    "react-datetime-range-picker": "^2.0.5",
    "react-dom": "^16.4.2",
    "react-google-maps": "^9.4.5",
    "react-image-gallery": "^0.8.16",
    "react-input-range": "^1.3.0",
    "react-mentions": "^2.4.1",
    "react-modal": "^3.6.1",
    "react-paginate": "^6.3.0",
    "react-redux": "^5.0.7",
    "react-redux-toastr": "^7.4.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^2.1.1",
    "react-select": "^2.0.0",
    "react-star-ratings": "^2.3.0",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "superagent": "^3.8.3"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "yarn build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start-js": "PORT=3009 react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "precommit": "lint-staged"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
